{
    "name": "librispeech_spexplus_test",
    "n_gpu": 1,
    "preprocessing": {
      "sr": 8000,
      "spectrogram": {
        "type": "MelSpectrogram",
        "args": {
        }
      },
      "log_spec": true
    },
    "augmentations": {
      "wave": [],
      "spectrogram": [
        {
          "type": "FreqMaskingSpecAug",
          "args": {
            "prob": 0.4,
            "max_freq_mask": 20
          }
        },
        {
          "type": "TimeMaskingSpecAug",
          "args": {
            "prob": 0.4,
            "max_time_mask": 100
          }
        },
        {
          "type": "TimeStretchSpecAug",
          "args": {
            "prob": 0.4,
            "stretch_min": 0.8,
            "stretch_max": 1.2,
            "n_freq": 128
          }
        }
      ]
    },
    "arch": {
      "type": "SpExPlusModel",
      "args": {
        "num_speakers": 101
      }
    },
    "data": {
      "train": {
        "batch_size": 10,
        "num_workers": 0,
        "datasets": [
          {
            "type": "LibrispeechMixedDataset",
            "args": {
              "part": "dev-clean",
              "max_audio_length": 20.0,
              "max_text_length": 200,
              "limit": 10
            }
          }
        ]
      },
      "val": {
        "batch_size": 10,
        "num_workers": 0,
        "datasets": [
          {
            "type": "LibrispeechMixedDataset",
            "args": {
              "part": "dev-clean",
              "max_audio_length": 20.0,
              "max_text_length": 200,
              "limit": 10
            }
          }
        ]
      }
    },
    "optimizer": {
      "type": "Adam",
      "args": {
        "lr": 3e-4
      }
    },
    "loss": {
      "type": "SpExPlusLoss",
      "args": {}
    },
    "metrics": [
    ],
    "rare_eval_metrics": {
    },
    "lr_scheduler": {
      "type": "CosineAnnealingWarmRestarts",
      "args": {
        "eta_min": 3e-5,
        "T_0": 2000
      }
    },
    "trainer": {
      "epochs": 50,
      "save_dir": "saved/",
      "save_period": 5,
      "verbosity": 2,
      "monitor": "min val_loss",
      "early_stop": 100,
      "visualize": "wandb",
      "wandb_project": "ss_project",
      "wandb_run_name": "one_batch_test",
      "len_epoch": 100,
      "grad_norm_clip": 10
    }
  }